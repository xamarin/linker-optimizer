<?xml version="1.0" encoding="utf-8" ?>
<linker-optimizer>
	<options all-modules="true" preprocessor="full" report-size="true" />

	<include filename="corlib-api.xml" />
	<include filename="corlib-asserts.xml" />

	<type name="X509Helper" action="fail" />
	<type name="X509Certificate" action="fail" />
	<type name="DependencyInjector" action="fail" />
	<type name="SystemDependencyProvider" action="fail" />

	<features>
		<feature name="security" enabled="false" />
	</features>
</linker-optimizer>
