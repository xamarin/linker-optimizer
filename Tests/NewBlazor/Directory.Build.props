<?xml version="1.0" encoding="utf-8"?>
<Project>

  <PropertyGroup>
    <!-- Keep in sync with the nuget.config in this folder. -->
    <Test_NuGet_GlobalsFolder>$([System.IO.Path]::GetFullPath('$(MSBuildThisFileDirectory)\.packages'))</Test_NuGet_GlobalsFolder>
    <!-- When set to true, we will always wipe the $(Test_NuGet_GlobalsFolder) prior to restoring. -->
    <Test_NuGet_ForceReinstall>true</Test_NuGet_ForceReinstall>
  </PropertyGroup>

</Project>
