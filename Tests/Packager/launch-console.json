{
   "version": "0.2.0",
   "configurations": [
        {
            "name": "@PROJECT@ Debug Build",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "debug-build",
            "program": "@ROOT@/Tests/DebugBuild/bin/Debug/netcoreapp3.0/DebugBuild.dll",
            "args": [
                "${workspaceFolder}"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "@PROJECT@ Optimizer",
            "type": "coreclr",
            "request": "launch",
            "program": "@ROOT@/Mono.Linker.Optimizer/bin/Debug/netcoreapp2.0/Mono.Linker.Optimizer.dll",
            "args": [
                "@aot/linker-in/linker-optimizer.response"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": true
        }
    ]
}
