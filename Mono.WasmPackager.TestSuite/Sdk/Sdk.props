<?xml version="1.0" encoding="utf-8"?>  
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <WasmPackager_EnablePackager>false</WasmPackager_EnablePackager>
    <_WasmPackager_SkipSdkImport>true</_WasmPackager_SkipSdkImport>
    <_WasmPackager_IsTestSuite>true</_WasmPackager_IsTestSuite>
  </PropertyGroup>

  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />

  <ItemGroup>
    <PackageReference Include="xunit" Version="2.4.0" />
    <PackageReference Include="xunit.runner.visualstudio" Version="2.4.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Mono.WasmPackager.TestSuite" Version="1.0.0" />
    <PackageReference Include="PuppeteerSharp" Version="2.0.0" />
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <Import Sdk="Mono.WasmPackager.DevServer" Version="1.0.0" Project="Sdk.props" />

</Project>
