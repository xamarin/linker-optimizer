<Project>

  <!-- Default Settings and Configuration -->

  <!-- Packager defaults -->
  <PropertyGroup>
    <WasmPackager_RuntimeTemplate Condition="'$(WasmPackager_RuntimeTemplate)' == ''">runtime-tests.js</WasmPackager_RuntimeTemplate>
    <WasmPackager_EnableDebug Condition="'$(WasmPackager_EnableDebug)' == ''">false</WasmPackager_EnableDebug>
    <WasmPackager_EmsdkCacheDir Condition="'$(WasmPackager_EmsdkCacheDir)' == ''">$([System.IO.Path]::GetFullPath('$(MSBuildProjectDirectory)/.emscripten-cache'))</WasmPackager_EmsdkCacheDir>
  </PropertyGroup>

  <!-- FIXME -->
  <ItemGroup>
    <!-- <WasmPackager_Profilers Include="TestProfiler" /> -->
    <WasmPackager_PInvokeLibraries Include="libfoo" />
  </ItemGroup>

  <!-- Packager settings -->
  <PropertyGroup>
    <WasmPackager_BuildDir>$([System.IO.Path]::GetFullPath('$(MSBuildProjectDirectory)/aot'))</WasmPackager_BuildDir>
    <WasmPackager_AppDir>$([System.IO.Path]::GetFullPath('$(MSBuildProjectDirectory)/app'))</WasmPackager_AppDir>
    <WasmPackager_VfsPrefix>managed</WasmPackager_VfsPrefix>
    <WasmPackager_DeployPrefix>managed</WasmPackager_DeployPrefix>
  </PropertyGroup>

</Project>
